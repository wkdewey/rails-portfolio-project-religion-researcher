<h1>Edit <%= @project.title %></h1>
<%= render partial: "application/errors", locals: { model: @project } %>
<%= render partial: "form", locals: { project: @project, texts: @texts } %>
<%= form_tag project_path(@project.id), method: "delete" do %>
  <%= submit_tag "Delete #{@project.title}" %>
<% end %>
